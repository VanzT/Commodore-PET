10 REM **********************************
20 REM * ICBM - COMMODORE PET VERSION    *
30 REM * POLAR PROJECTION NUCLEAR WAR    *
40 REM **********************************
100 REM INITIALIZATION
110 DIM M$(100,6):REM MISSILE DATA: HEX,TYPE,DUR,MOVE,STR,OWNER,STATUS
120 DIM A$(12,4):REM ABM DATA: HEX,OWNER,ROUNDS,SITE LETTER
130 DIM C$(50,3):REM CITY DATA: HEX,VALUE,OWNER
140 DIM B$(30,2):REM BASE DATA: HEX,MISSILE COUNT
150 MC=0:AC=0:CC=0:BC=0:REM COUNTERS
160 UP=0:SP=0:REM USSR POINTS, USA POINTS
170 TURN=0:REM TURN COUNTER
180 GOSUB 10000:REM LOAD CITY DATA
190 PRINT CHR$(147):REM CLEAR SCREEN
200 PRINT "=================================="
210 PRINT "I.C.B.M."
220 PRINT "INTERCONTINENTAL BALLISTIC MISSILE"
230 PRINT "=================================="
240 PRINT
250 PRINT "PRESS ANY KEY TO BEGIN SETUP..."
260 GET K$:IF K$="" THEN 260
300 REM ===== SETUP PHASE =====
310 PRINT CHR$(147)
320 PRINT "===== SETUP PHASE ====="
330 PRINT
340 PRINT "YOU ARE USA (BLUE COUNTERS)"
350 PRINT "COMPUTER IS USSR (RED COUNTERS)"
360 PRINT
370 PRINT "USSR SETS UP FIRST..."
380 GOSUB 5000:REM USSR AI SETUP
390 PRINT
400 PRINT "NOW PLACE YOUR FORCES:"
410 GOSUB 6000:REM USA PLAYER SETUP
500 REM ===== FIRST LAUNCH BID =====
510 PRINT CHR$(147)
520 PRINT "===== FIRST LAUNCH BID ====="
530 PRINT
540 PRINT "BID VICTORY POINTS TO GO FIRST"
550 PRINT "(OPPONENT RECEIVES YOUR BID)"
560 PRINT
570 INPUT "YOUR BID (0-20)";UB
580 CB=INT(RND(1)*15):REM COMPUTER BIDS 0-14
590 PRINT
600 PRINT "COMPUTER BIDS";CB;"POINTS"
610 PRINT
620 IF UB>CB THEN FIRST=1:SP=SP-UB:PRINT "YOU LAUNCH FIRST!":GOTO 650
630 FIRST=2:UP=UP-CB:PRINT "USSR LAUNCHES FIRST!"
650 PRINT:PRINT "PRESS ANY KEY TO START WAR..."
660 GET K$:IF K$="" THEN 660
700 REM ===== MAIN GAME LOOP =====
710 PRINT CHR$(147)
720 TURN=TURN+1
730 PRINT "===== TURN";TURN;"====="
740 PRINT
750 IF FIRST=1 THEN GOSUB 7000:GOSUB 8000:GOTO 770
760 GOSUB 8000:GOSUB 7000
770 REM CHECK IF GAME OVER
780 GOSUB 9500:REM CHECK FOR ACTIVE MISSILES
790 IF AM=0 AND NL=0 THEN GOTO 900
800 PRINT:PRINT "PRESS ANY KEY FOR NEXT TURN..."
810 GET K$:IF K$="" THEN 810
820 GOTO 710
900 REM ===== GAME OVER =====
910 PRINT CHR$(147)
920 PRINT "===== GAME OVER ====="
930 GOSUB 9000:REM CALCULATE VICTORY
940 END
5000 REM ***** USSR AI SETUP *****
5010 PRINT "USSR DEPLOYING FORCES..."
5020 REM DEPLOY MISSILES IN BASES
5030 REM BASE 1: MOSCOW AREA (0426)
5040 GOSUB 5500:REM CREATE BASE
5050 B$(BC,0)="0426":B$(BC,1)="15"
5060 FOR I=1 TO 15
5070 MC=MC+1
5080 M$(MC,0)="0426":M$(MC,5)="2":M$(MC,6)="0":REM HEX,OWNER=USSR,INACTIVE
5090 IF I<=3 THEN M$(MC,1)="SS18":M$(MC,2)="4":M$(MC,3)="10":M$(MC,4)="8"
5100 IF I>3 AND I<=6 THEN M$(MC,1)="SS9":M$(MC,2)="4":M$(MC,3)="10":M$(MC,4)="6"
5110 IF I>6 AND I<=9 THEN M$(MC,1)="SS19":M$(MC,2)="4":M$(MC,3)="8":M$(MC,4)="5"
5120 IF I>9 AND I<=15 THEN M$(MC,1)="SS11":M$(MC,2)="4":M$(MC,3)="8":M$(MC,4)="2"
5130 NEXT I
5140 REM BASE 2: URALS (0926)
5150 GOSUB 5500
5160 B$(BC,0)="0926":B$(BC,1)="12"
5170 FOR I=1 TO 12
5180 MC=MC+1
5190 M$(MC,0)="0926":M$(MC,5)="2":M$(MC,6)="0"
5200 IF I<=2 THEN M$(MC,1)="SS18":M$(MC,2)="4":M$(MC,3)="10":M$(MC,4)="8"
5210 IF I>2 AND I<=5 THEN M$(MC,1)="SS19":M$(MC,2)="4":M$(MC,3)="8":M$(MC,4)="5"
5220 IF I>5 AND I<=12 THEN M$(MC,1)="SS11":M$(MC,2)="4":M$(MC,3)="8":M$(MC,4)="2"
5230 NEXT I
5240 REM BASE 3: SIBERIA (1330)
5250 GOSUB 5500
5260 B$(BC,0)="1330":B$(BC,1)="10"
5270 FOR I=1 TO 10
5280 MC=MC+1
5290 M$(MC,0)="1330":M$(MC,5)="2":M$(MC,6)="0"
5300 IF I<=2 THEN M$(MC,1)="SS18":M$(MC,2)="4":M$(MC,3)="10":M$(MC,4)="8"
5310 IF I>2 AND I<=5 THEN M$(MC,1)="SS19":M$(MC,2)="4":M$(MC,3)="8":M$(MC,4)="5"
5320 IF I>5 AND I<=10 THEN M$(MC,1)="SS11":M$(MC,2)="4":M$(MC,3)="8":M$(MC,4)="2"
5330 NEXT I
5340 REM BASE 4: FAR EAST (2632)
5350 GOSUB 5500
5360 B$(BC,0)="2632":B$(BC,1)="12"
5370 FOR I=1 TO 12
5380 MC=MC+1
5390 M$(MC,0)="2632":M$(MC,5)="2":M$(MC,6)="0"
5400 IF I<=3 THEN M$(MC,1)="SS18":M$(MC,2)="4":M$(MC,3)="10":M$(MC,4)="8"
5410 IF I>3 AND I<=5 THEN M$(MC,1)="SS19":M$(MC,2)="4":M$(MC,3)="8":M$(MC,4)="5"
5420 IF I>5 AND I<=7 THEN M$(MC,1)="SS17":M$(MC,2)="4":M$(MC,3)="8":M$(MC,4)="3"
5430 IF I>7 AND I<=12 THEN M$(MC,1)="SS11":M$(MC,2)="4":M$(MC,3)="8":M$(MC,4)="2"
5440 NEXT I
5450 REM DEPLOY ABM SITES
5460 GOSUB 5600:A$(AC,0)="0426":A$(AC,1)="2":A$(AC,2)="5":A$(AC,3)="A"
5470 GOSUB 5600:A$(AC,0)="0530":A$(AC,1)="2":A$(AC,2)="4":A$(AC,3)="B"
5480 GOSUB 5600:A$(AC,0)="0826":A$(AC,1)="2":A$(AC,2)="3":A$(AC,3)="C"
5490 GOSUB 5600:A$(AC,0)="1130":A$(AC,1)="2":A$(AC,2)="4":A$(AC,3)="D"
5492 GOSUB 5600:A$(AC,0)="1330":A$(AC,1)="2":A$(AC,2)="3":A$(AC,3)="E"
5494 GOSUB 5600:A$(AC,0)="2632":A$(AC,1)="2":A$(AC,2)="4":A$(AC,3)="F"
5496 PRINT "USSR DEPLOYED 4 BASES, 6 ABM SITES"
5498 RETURN
5500 BC=BC+1:RETURN:REM INCREMENT BASE COUNTER
5600 AC=AC+1:RETURN:REM INCREMENT ABM COUNTER
6000 REM ***** USA PLAYER SETUP *****
6010 PRINT
6020 PRINT "YOU HAVE 42 MISSILES TO DEPLOY:"
6030 PRINT "  2 x TT2  (4-9 STR7)"
6040 PRINT " 22 x MM3  (4-9 STR6)"
6050 PRINT " 18 x MM2  (4-9 STR3)"
6060 PRINT
6070 PRINT "SUGGESTED USA HEXES:"
6080 PRINT "  MONTANA: 0917"
6090 PRINT "  MIDWEST: 1215"
6100 PRINT "  PLAINS:  1319"
6110 PRINT
6120 INPUT "HOW MANY BASES (2-4)";NB
6130 IF NB<2 OR NB>4 THEN 6120
6140 FOR I=1 TO NB
6150 PRINT:PRINT "BASE";I
6160 INPUT "  HEX";BH$
6170 INPUT "  # MISSILES (10-20)";BM
6180 GOSUB 5500:REM INCREMENT BASE COUNTER
6190 B$(BC,0)=BH$:B$(BC,1)=STR$(BM)
6200 REM ASSIGN MISSILE TYPES
6210 TT2=2:MM3=22:MM2=18:REM REMAINING
6220 FOR J=1 TO BM
6230 MC=MC+1
6240 M$(MC,0)=BH$:M$(MC,5)="1":M$(MC,6)="0":REM OWNER=USA
6250 IF TT2>0 THEN M$(MC,1)="TT2":M$(MC,2)="4":M$(MC,3)="9":M$(MC,4)="7":TT2=TT2-1:GOTO 6290
6260 IF MM3>0 THEN M$(MC,1)="MM3":M$(MC,2)="4":M$(MC,3)="9":M$(MC,4)="6":MM3=MM3-1:GOTO 6290
6270 IF MM2>0 THEN M$(MC,1)="MM2":M$(MC,2)="4":M$(MC,3)="9":M$(MC,4)="3":MM2=MM2-1:GOTO 6290
6280 PRINT "ERROR: OUT OF MISSILES"
6290 NEXT J
6300 NEXT I
6310 REM DEPLOY ABM SITES
6320 PRINT:PRINT "DEPLOY 5 ABM SITES (A-E):"
6330 FOR I=1 TO 5
6340 PRINT:PRINT "ABM SITE ";CHR$(64+I)
6350 INPUT "  HEX";AH$
6360 INPUT "  ROUNDS (2-8)";AR
6370 GOSUB 5600
6380 A$(AC,0)=AH$:A$(AC,1)="1":A$(AC,2)=STR$(AR):A$(AC,3)=CHR$(64+I)
6390 SP=SP-(AR*2):REM DEDUCT 2 VP PER ABM ROUND
6400 NEXT I
6410 RETURN
7000 REM ***** USA PLAYER TURN *****
7010 PRINT CHR$(147)
7020 PRINT "===== USA TURN";TURN;"====="
7030 PRINT
7040 REM ACTIVATION ROLL
7050 R=INT(RND(1)*6)+1
7060 ACT=6+(R*2):REM USA ACTIVATION TABLE
7070 PRINT "ACTIVATION ROLL:";R
7080 PRINT "CAN ACTIVATE:";ACT;"MISSILES"
7090 PRINT
7100 GOSUB 7500:REM PLAYER ACTIVATES & MOVES
7110 RETURN
7500 REM PLAYER MISSILE ACTIVATION & MOVEMENT
7510 INPUT "ACTIVATE HOW MANY";NA
7520 IF NA>ACT THEN PRINT "TOO MANY!":GOTO 7510
7530 IF NA=0 THEN RETURN
7540 FOR I=1 TO NA
7550 PRINT:PRINT "ACTIVATE MISSILE";I;"OF";NA
7560 INPUT "  FROM HEX";FH$
7570 REM FIND INACTIVE MISSILE AT THIS HEX
7580 MID=-1
7590 FOR J=1 TO MC
7600 IF M$(J,0)=FH$ AND M$(J,5)="1" AND M$(J,6)="0" THEN MID=J:J=MC
7610 NEXT J
7620 IF MID=-1 THEN PRINT "NO MISSILE AT";FH$:GOTO 7560
7630 REM ACTIVATE THIS MISSILE
7640 M$(MID,6)="1":REM ACTIVE
7650 PRINT "  ACTIVATED ";M$(MID,1);" (DUR:";M$(MID,2);" MV:";M$(MID,3);" STR:";M$(MID,4);")"
7660 PRINT
7670 GOSUB 7700:REM MOVE THIS MISSILE
7680 NEXT I
7690 RETURN
7700 REM MOVE ONE MISSILE
7710 PRINT "  MOVE TO HEX (0000 TO DELAY)";
7720 INPUT TH$
7730 IF TH$="0000" THEN RETURN
7740 REM VALIDATE MOVE (SIMPLE RANGE CHECK)
7750 GOSUB 7900:REM CALCULATE HEX DISTANCE
7760 IF DIST>VAL(M$(MID,3)) THEN PRINT "  TOO FAR! MAX:";M$(MID,3):GOTO 7710
7770 REM CHECK USSR ABM INTERCEPTION
7780 M$(MID,0)=TH$:REM UPDATE POSITION
7790 GOSUB 8500:REM CHECK ABM FIRE
7800 REM ASK ABOUT IMPACT
7810 IF M$(MID,6)="9" THEN RETURN:REM DESTROYED
7820 INPUT "  IMPACT HERE? (Y/N)";IM$
7830 IF IM$="Y" THEN GOSUB 7950:REM RESOLVE IMPACT
7840 RETURN
7900 REM CALCULATE HEX DISTANCE (SIMPLIFIED)
7910 FX=VAL(LEFT$(M$(MID,0),2)):FY=VAL(RIGHT$(M$(MID,0),2))
7920 TX=VAL(LEFT$(TH$,2)):TY=VAL(RIGHT$(TH$,2))
7930 DIST=ABS(TX-FX)+ABS(TY-FY):REM MANHATTAN DISTANCE APPROXIMATION
7940 RETURN
7950 REM RESOLVE MISSILE IMPACT
7960 PRINT "  ** IMPACT **"
7970 REM CHECK IF CITY HIT
7980 FOR J=1 TO CC
7990 IF C$(J,0)=TH$ THEN CV=VAL(C$(J,1)):CX=VAL(M$(MID,4)):C$(J,1)=STR$(CV-CX):SP=SP+CX:PRINT "  HIT CITY! -";CX;"PTS"
8000 NEXT J
8010 M$(MID,6)="8":REM IMPACTED
8020 RETURN
8000 REM ***** USSR AI TURN *****
8010 PRINT CHR$(147)
8020 PRINT "===== USSR TURN";TURN;"====="
8030 PRINT
8040 REM ACTIVATION ROLL
8050 IF TURN=1 THEN ACT=12:PRINT "FIRST TURN: 12 MISSILES":GOTO 8080
8060 R=INT(RND(1)*6)+1
8070 ACT=8+(R*2):REM USSR ACTIVATION TABLE
8075 PRINT "USSR ACTIVATION ROLL:";R
8080 PRINT "USSR ACTIVATES:";ACT;"MISSILES"
8090 PRINT
8100 GOSUB 8200:REM AI ACTIVATES & MOVES
8110 PRINT "PRESS ANY KEY..."
8120 GET K$:IF K$="" THEN 8120
8130 RETURN
8200 REM AI MISSILE ACTIVATION & MOVEMENT
8210 NA=0:REM ACTIVATED COUNT
8220 FOR I=1 TO MC
8230 IF M$(I,5)="2" AND M$(I,6)="0" AND NA<ACT THEN GOSUB 8300:NA=NA+1:REM ACTIVATE USSR MISSILE
8240 NEXT I
8250 RETURN
8300 REM ACTIVATE & MOVE ONE AI MISSILE
8310 M$(I,6)="1":REM ACTIVATE
8320 PRINT "USSR LAUNCHES ";M$(I,1);" FROM ";M$(I,0)
8330 REM AI PICKS TARGET (HIGHEST VALUE USA CITY)
8340 TARG$="":TVAL=0
8350 FOR J=1 TO CC
8360 IF C$(J,2)="1" AND VAL(C$(J,1))>TVAL THEN TARG$=C$(J,0):TVAL=VAL(C$(J,1))
8370 NEXT J
8380 IF TARG$="" THEN TARG$="1215":REM DEFAULT TARGET
8390 PRINT "  TARGET: ";TARG$
8400 M$(I,0)=TARG$:REM MOVE TO TARGET
8410 GOSUB 8550:REM CHECK USA ABM (PLAYER CONTROLLED)
8420 IF M$(I,6)="9" THEN RETURN:REM DESTROYED
8430 REM IMPACT
8440 PRINT "  ** IMPACT **"
8450 FOR J=1 TO CC
8460 IF C$(J,0)=TARG$ THEN CV=VAL(C$(J,1)):CX=VAL(M$(I,4)):C$(J,1)=STR$(CV-CX):UP=UP+CX:PRINT "  HIT ";TARG$;" -";CX;"PTS"
8470 NEXT J
8480 M$(I,6)="8":REM IMPACTED
8490 RETURN
8500 REM CHECK USSR ABM FIRE AT USA MISSILE
8510 PRINT "  USSR ABM CHECK..."
8520 REM AI SIMPLE: 50% CHANCE TO FIRE IF IN RANGE
8530 IF RND(1)<.5 THEN PRINT "  NO ABM FIRE":RETURN
8540 GOSUB 8600:REM ATTEMPT ABM SHOT
8545 RETURN
8550 REM CHECK USA ABM FIRE AT USSR MISSILE
8560 PRINT "  USA ABM CHECK..."
8570 INPUT "  FIRE ABM? (Y/N)";AF$
8580 IF AF$<>"Y" THEN RETURN
8590 GOSUB 8600:REM ATTEMPT ABM SHOT
8595 RETURN
8600 REM PERFORM ABM SHOT
8610 REM FIND NEAREST ABM SITE WITH ROUNDS
8620 OWNER=VAL(M$(I,5)):REM MISSILE OWNER
8630 DEF=3-OWNER:REM DEFENDER (1->2, 2->1)
8640 NEAR=-1:NDIST=999
8650 FOR J=1 TO AC
8660 IF A$(J,1)=STR$(DEF) AND VAL(A$(J,2))>0 THEN GOSUB 8750:IF ADIST<NDIST THEN NEAR=J:NDIST=ADIST
8670 NEXT J
8680 IF NEAR=-1 THEN PRINT "  NO ABM AVAILABLE":RETURN
8690 IF NDIST>3 THEN PRINT "  OUT OF RANGE":RETURN
8700 REM FIRE ABM
8710 A$(NEAR,2)=STR$(VAL(A$(NEAR,2))-1):REM USE ONE ROUND
8720 PRINT "  ABM SITE ";A$(NEAR,3);" FIRES! (";NDIST;" HEXES)"
8730 GOSUB 8800:REM RESOLVE ABM SHOT
8740 RETURN
8750 REM CALCULATE DISTANCE FROM ABM TO MISSILE
8760 AX=VAL(LEFT$(A$(J,0),2)):AY=VAL(RIGHT$(A$(J,0),2))
8770 MX=VAL(LEFT$(M$(I,0),2)):MY=VAL(RIGHT$(M$(I,0),2))
8780 ADIST=ABS(AX-MX)+ABS(AY-MY)
8790 RETURN
8800 REM RESOLVE ABM SHOT (USE TABLE FROM RULES)
8810 R=INT(RND(1)*6)+1
8820 HIT=0
8830 IF NDIST=1 THEN HIT=1:REM ALWAYS HIT AT RANGE 1
8840 IF NDIST=2 AND R<=4 THEN HIT=1:REM HIT ON 1-4
8850 IF NDIST=3 AND R<=2 THEN HIT=1:REM HIT ON 1-2
8860 PRINT "  ROLL:";R;
8870 IF HIT=1 THEN PRINT " **HIT**":M$(I,6)="9":RETURN
8880 PRINT " MISS":RETURN
9000 REM ***** CALCULATE VICTORY *****
9010 PRINT
9020 PRINT "===== FINAL SCORE ====="
9030 PRINT
9040 REM COUNT SURVIVING MISSILES
9050 FOR I=1 TO MC
9060 IF M$(I,6)="0" AND M$(I,5)="1" THEN SP=SP+1
9070 IF M$(I,6)="0" AND M$(I,5)="2" THEN UP=UP+1
9080 NEXT I
9090 PRINT "USA POINTS:";SP
9100 PRINT "USSR POINTS:";UP
9110 PRINT
9120 DIFF=ABS(SP-UP)
9130 IF SP>UP THEN PRINT "*** USA WINS ***":GOTO 9170
9140 IF UP>SP THEN PRINT "*** USSR WINS ***":GOTO 9170
9150 PRINT "*** DRAW ***":RETURN
9170 IF DIFF<10 THEN PRINT "MARGINAL VICTORY":RETURN
9180 IF DIFF<20 THEN PRINT "SUBSTANTIAL VICTORY":RETURN
9190 PRINT "DECISIVE VICTORY":RETURN
9500 REM CHECK FOR ACTIVE MISSILES OR LAUNCH CAPABILITY
9510 AM=0:NL=0
9520 FOR I=1 TO MC
9530 IF M$(I,6)="1" THEN AM=AM+1:REM ACTIVE MISSILES IN AIR
9540 IF M$(I,6)="0" THEN NL=NL+1:REM CAN STILL LAUNCH
9550 NEXT I
9560 RETURN
10000 REM ***** LOAD CITY DATA *****
10010 REM FORMAT: HEX, VALUE, OWNER (1=USA, 2=USSR)
10020 REM USA CITIES
10030 CC=CC+1:C$(CC,0)="1007":C$(CC,1)="5":C$(CC,2)="1":REM NEW YORK
10040 CC=CC+1:C$(CC,0)="1106":C$(CC,1)="3":C$(CC,2)="1":REM PHILADELPHIA
10050 CC=CC+1:C$(CC,0)="1008":C$(CC,1)="5":C$(CC,2)="1":REM BOSTON
10060 CC=CC+1:C$(CC,0)="1107":C$(CC,1)="3":C$(CC,2)="1":REM WASHINGTON DC
10070 CC=CC+1:C$(CC,0)="1208":C$(CC,1)="3":C$(CC,2)="1":REM TORONTO
10080 CC=CC+1:C$(CC,0)="1209":C$(CC,1)="5":C$(CC,2)="1":REM MONTREAL
10090 CC=CC+1:C$(CC,0)="1308":C$(CC,1)="5":C$(CC,2)="1":REM CHICAGO
